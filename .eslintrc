{
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "curly": [2, "multi-line"],
    "no-shadow": 0,
    "no-trailing-spaces": 0,
    "no-underscore-dangle": 0,
    "no-unused-expressions": 0,
    "quotes": [2, "single", "avoid-escape"],
    "semi": 2,
    "no-unused-vars": 1,
    "strict": 0
  }
}
